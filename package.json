{
  "name": "ev-jade",
  "version": "0.0.11",
  "author": "zppro",
  "description": "养老平台-护士站",
  "license": "GPL-3.0",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:splasher && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:splasher": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.splasher.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run compatible && npm run lint:fix",
    "compatible": "npm run compatible:bulma-gte-0.6.0",
    "compatible:bulma-gte-0.6.0": "cp compatible/bulma/gte-0.6.0/variables.sass node_modules/bulma/sass/utilities/variables.sass"
  },
  "build": {
    "productName": "ev-jade",
    "appId": "org.woosiyuan.ev-jade",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "axios": "^0.16.1",
    "bulma": "^0.6.1",
    "bulma-badge": "^0.1.0",
    "bulma-tooltip": "^0.1.1",
    "font-awesome": "4.7.0",
    "nsqjs": "^0.10.0",
    "rand-token": "^0.4.0",
    "vue": "^2.3.3",
    "vue-bulma-brace": "^0.1.0",
    "vue-bulma-card": "^1.0.2",
    "vue-bulma-chartist": "^1.1.0",
    "vue-bulma-chartjs": "^1.0.5",
    "vue-bulma-collapse": "^1.0.3",
    "vue-bulma-datepicker": "^1.3.6",
    "vue-bulma-expanding": "^0.0.1",
    "vue-bulma-jump": "^0.0.2",
    "vue-bulma-message": "^1.1.1",
    "vue-bulma-modal": "1.0.1",
    "vue-bulma-notification": "^1.1.1",
    "vue-bulma-progress-bar": "^1.0.2",
    "vue-bulma-progress-tracker": "0.0.4",
    "vue-bulma-quill": "^0.0.1",
    "vue-bulma-rating": "^1.0.1",
    "vue-bulma-slider": "^1.0.2",
    "vue-bulma-switch": "^2.0.0-alpha.2",
    "vue-bulma-tabs": "^1.1.2",
    "vue-bulma-tooltip": "^1.0.4",
    "vue-cleave": "^1.2.2",
    "vue-electron": "^1.0.6",
    "vue-lory": "^0.0.5",
    "vue-nprogress": "^0.1.5",
    "vue-peity": "^0.5.0",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^5.0.0",
    "wysiwyg.css": "^0.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.7.5",
    "electron-builder": "^19.19.1",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "inject-loader": "^3.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.0.2",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.7.2",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "require-dir": "^0.3.0",
    "sass-loader": "^6.0.6",
    "spectron": "^3.7.1",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  }
}
